{
  "record": {
    "$": { "id": "delivery_order_search", "model": "ir.ui.view" },
    "field": [
      { "_": "出运要求搜索", "$": { "name": "name" } },
      { "_": "delivery.order", "$": { "name": "model" } },
      {
        "$": { "name": "arch", "type": "xml" },
        "search": {
          "$": { "string": "出运要求" },
          "field": [
            { "$": { "name": "container_num" } },
            { "$": { "name": "vessl_operator" } }
          ],
          "separator": ["", "", "", ""],
          "filter": [
            {
              "$": { "name": "date_ATD", "string": "实际船期", "date": "ATD" }
            },
            {
              "$": {
                "name": "pack_status_draft",
                "string": "包装状态-草拟",
                "domain": "[('pack_status', '=', 'draft')]"
              }
            },
            {
              "$": {
                "name": "pack_status_submit",
                "string": "包装状态-提交",
                "domain": "[('pack_status', '=', 'submit')]"
              }
            },
            {
              "$": {
                "name": "pack_status_confirm",
                "string": "包装状态-确认",
                "domain": "[('pack_status', '=', 'confirm')]"
              }
            },
            {
              "$": {
                "name": "undelivered_all_cnt_1",
                "string": "待出数量 > 0",
                "domain": "[('undelivered_all_cnt', '>', 0)]"
              }
            },
            {
              "$": {
                "name": "undelivered_all_cnt_0",
                "string": "待出数量 = 0",
                "domain": "[('undelivered_all_cnt', '=', 0)]"
              }
            }
          ],
          "group": {
            "$": { "expand": "1", "string": "分组" },
            "filter": [
              {
                "$": {
                  "name": "group_customer_id",
                  "string": "客户",
                  "context": "{'group_by':'customer_id'}"
                }
              },
              {
                "$": {
                  "name": "group_loading_port",
                  "string": "出运港",
                  "context": "{'group_by':'loading_port'}"
                }
              }
            ]
          }
        }
      }
    ]
  }
}
