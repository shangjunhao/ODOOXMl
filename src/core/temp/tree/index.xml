<!-- tree -->
<record :id="tree.id" model="ir.ui.view">
    <field name="name">{{tree.name}}</field>
    <field name="model">{{model._modelName}}</field>
    <field name="arch" type="xml">
        <tree :string="tree.name" :create="tree.create" :edit="tree.edit" :delete="tree.del" :default_order="tree.order" v-has:editable="form">
            <!-- 方法一 -->
            {{tree.fields}}
            <!-- 方法二 子组件渲染 -->
            <!-- <v-field v-for="item in tree.fields"></v-field> -->
        </tree>
    </field>
</record>